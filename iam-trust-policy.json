{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::<AWS_ACCOUNT_ID>:oidc-provider/vstoken.dev.azure.com/<ORGANIZATION_ID>"
      },
      "Action": "sts:AssumeRoleWithWebIdentity",
      "Condition": {
        "StringEquals": {
          "vstoken.dev.azure.com/<ORGANIZATION_ID>:aud": "sts.amazonaws.com",
          "vstoken.dev.azure.com/<ORGANIZATION_ID>:sub": "sc://2286671-POC/AWS-ADO/*"
        }
      }
    }
  ]
}
